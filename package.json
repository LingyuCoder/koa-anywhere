{
  "name": "koa-anywhere",
  "version": "0.1.0",
  "engines": {
    "node": ">= 4.0.0"
  },
  "bin": {
    "ka": "./bin/ka.js"
  },
  "description": "koa-anywhere==>通过一条命令，将当前目录变成一个静态文件服务器",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/_mocha --require should --reporter spec",
    "test-cov": "NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require should",
    "test-travis": "NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --require should"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/LingyuCoder/koa-anywhere.git"
  },
  "author": {
    "name": "skyinlayer"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LingyuCoder/koa-anywhere/issues"
  },
  "homepage": "https://github.com/LingyuCoder/koa-anywhere",
  "dependencies": {
    "colors": "~1.1.2",
    "commander": "~2.9.0",
    "debug": "~2.2.0",
    "jade": "~1.11.0",
    "koa": "~1.1.2",
    "koa-static": "~1.5.2",
    "lodash": "~3.10.1",
    "open": "0.0.5",
    "optimist": "~0.6.1"
  },
  "main": "index.js",
  "devDependencies": {
    "eslint": "~1.9.0",
    "istanbul": "~0.4.0",
    "mocha": "~2.3.3",
    "should": "~7.1.1"
  },
  "keywords": [
    "koa",
    "file",
    "static",
    "server",
    "anywhere"
  ]
}
